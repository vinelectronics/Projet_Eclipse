 108:main.c        **** int c_entry(void)
 109:main.c        **** {
 110:main.c        **** 	FlagStatus exitflag;
 111:main.c        **** 	char key;
 112:main.c        **** 
 113:main.c        **** 	// DeInit NVIC and SCBNVIC
 114:main.c        **** 	NVIC_DeInit();
 115:main.c        **** 	NVIC_SCBDeInit();
 116:main.c        **** 
 117:main.c        **** 	/* Configure the NVIC Preemption Priority Bits:
 118:main.c        **** 	 * two (2) bits of preemption priority, six (6) bits of sub-priority.
 119:main.c        **** 	 * Since the Number of Bits used for Priority Levels is five (5), so the
 120:main.c        **** 	 * actual bit number of sub-priority is three (3)
 121:main.c        **** 	 */
 122:main.c        **** 	NVIC_SetPriorityGrouping(0x05);
 123:main.c        **** 
 124:main.c        **** 	//  Set Vector table offset value
 125:main.c        **** #if (__RAM_MODE__==1)
 126:main.c        **** 	NVIC_SetVTOR(0x10000000);
 127:main.c        **** #else
 128:main.c        **** 	NVIC_SetVTOR(0x00000000);
 129:main.c        **** #endif
 130:main.c        **** 
 131:main.c        **** 	init_in_main();
 132:main.c        **** 	LED1_on();
 133:main.c        **** 
 134:main.c        **** 	comm_init();
 135:main.c        **** 
 136:main.c        **** 	comm_puts("Hello from the ChaN FatFs Demo on LPC1700\r\n");
 137:main.c        **** 	comm_puts("Version " VERSION_STRING ", Martin Thomas 7/2010\r\n");
 138:main.c        **** #ifdef DEBUG
 139:main.c        **** 	comm_puts("DEBUG build\r\n");
 140:main.c        **** #endif
 141:main.c        **** 
 142:main.c        **** 	xfunc_out = xcomm_put;
 143:main.c        **** 	xfunc_in  = xcomm_get;
 144:main.c        **** 	xprintf("xprintf is working\n");
 145:main.c        **** 
 146:main.c        **** 
 147:main.c        **** 	SysTick_Config(SystemCoreClock/1000 - 1); /* Generate interrupt each 1 ms   */
 148:main.c        **** 
 149:main.c        **** #if 0
 150:main.c        **** 	usb_test1();
 151:main.c        **** #endif
 152:main.c        **** 
 153:main.c        **** 	// reset exit flag
 154:main.c        **** 	// exitflag = RESET;
 155:main.c        **** 	exitflag = SET;
 156:main.c        **** 
 157:main.c        **** 	/* Read some data from the buffer */
 158:main.c        **** 	while (exitflag == RESET)
 159:main.c        **** 	{
 160:main.c        **** 
 161:main.c        **** 		if ( comm_test() ) {
 162:main.c        **** 			key = comm_get();
 163:main.c        **** 			if (key == 27) {
 164:main.c        **** 				/* ESC key, set exit flag */
 165:main.c        **** 				comm_puts("Exiting...\r\n");
 166:main.c        **** 				exitflag = SET;
 167:main.c        **** 			} else {
 168:main.c        **** 				/* Echo it back */
 169:main.c        **** 				comm_put(key);
 170:main.c        **** 			}
 171:main.c        **** 		}
 172:main.c        **** 	}
 173:main.c        **** 
 174:main.c        **** 	// wait for current transmission complete - THR must be empty
 175:main.c        **** 	while ( comm_txbusy() ) { ; }
 176:main.c        **** 
 177:main.c        **** 	ff_test_term();
 178:main.c        **** 
 179:main.c        **** 	/* Loop forever */
 180:main.c        **** 	while (1)
 181:main.c        **** 		;
 182:main.c        **** 	return 1;
 183:main.c        **** }
