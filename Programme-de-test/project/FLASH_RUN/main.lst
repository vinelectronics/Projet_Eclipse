  39:main.c        **** // LED1 on Olimex LPC1766-STK connected to P1.25 (low = on)
  40:main.c        **** #define LED1_PORT  1
  41:main.c        **** #define LED1_PIN   25
  42:main.c        **** #define LED1_MASK (1 << LED1_PIN)
  43:main.c        **** 
  44:main.c        **** volatile uint32_t SysTickCnt;      /* SysTick Counter                    */
  45:main.c        **** 
  46:main.c        **** /************************** PRIVATE FUNCTIONS *************************/
  47:main.c        **** 
  48:main.c        **** static void init_in_main(void)
  49:main.c        **** {
  50:main.c        **** 	RTC_TIME_Type initial_time = {
  51:main.c        **** 		0, 55, 11, // SS:MM:HH
  52:main.c        **** 		15, 0, 0,  // DOM, DOW, DOY
  53:main.c        **** 		7, 2010    // MONTH, YEAR
  54:main.c        **** 	};
  55:main.c        **** 
  56:main.c        **** 	/* LED(s) */
  57:main.c        **** 	PINSEL_CFG_Type PinCfg;
  58:main.c        **** 
  59:main.c        **** 	PinCfg.Funcnum   = 0;
  60:main.c        **** 	PinCfg.OpenDrain = 0;
  61:main.c        **** 	PinCfg.Pinmode   = 0;
  62:main.c        **** 	PinCfg.Pinnum    = LED1_PIN;
  63:main.c        **** 	PinCfg.Portnum   = LED1_PORT;
  64:main.c        **** 	PINSEL_ConfigPin(&PinCfg);
  65:main.c        **** 	GPIO_SetDir(1, LED1_MASK, 1);
  66:main.c        **** 
  67:main.c        **** 	/* RTC */
  68:main.c        **** 	rtc_cal_init(&initial_time);
  69:main.c        **** }
  70:main.c        **** 
  71:main.c        **** static inline void LED1_on(void)
  72:main.c        **** {
  73:main.c        **** 	FIO_ClearValue(LED1_PORT, LED1_MASK);
  74:main.c        **** }
  75:main.c        **** 
  76:main.c        **** static inline void LED1_off(void)
  77:main.c        **** {
  78:main.c        **** 	FIO_SetValue(LED1_PORT, LED1_MASK);
  79:main.c        **** }
  80:main.c        **** 
  81:main.c        **** /* SysTick Interrupt Handler (1ms) */
  82:main.c        **** void SysTick_Handler (void)
  83:main.c        **** {
  84:main.c        **** 	static uint32_t flip, led_prescale;
  85:main.c        **** 	static uint32_t div10;
  86:main.c        **** 
  87:main.c        **** 	if (++led_prescale > 500) {
  88:main.c        **** 		led_prescale = 0;
  89:main.c        **** 		if (flip) { LED1_on();  }
  90:main.c        **** 		else      { LED1_off(); }
  91:main.c        **** 		flip = !flip;
  92:main.c        **** 	}
  93:main.c        **** 
  94:main.c        **** 	if (++div10 >= 10) {
  95:main.c        **** 		div10 = 0;
  96:main.c        **** 		disk_timerproc(); /* Disk timer function (100Hz) */
  97:main.c        **** 	}
  98:main.c        **** 
  99:main.c        **** 	ff_test_term_timer_callback();
 100:main.c        **** 
 101:main.c        **** 	SysTickCnt++;
 102:main.c        **** }
