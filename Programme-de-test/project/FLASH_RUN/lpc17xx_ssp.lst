 292:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @brief		Initializes the SSPx peripheral according to the specified
 293:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** *               parameters in the SSP_ConfigStruct.
 294:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @param[in]	SSPx	SSP peripheral selected, should be SSP0 or SSP1
 295:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @param[in]	SSP_ConfigStruct Pointer to a SSP_CFG_Type structure
 296:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** *                    that contains the configuration information for the
 297:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** *                    specified SSP peripheral.
 298:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @return 		None
 299:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  *********************************************************************/
 300:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** void SSP_Init(LPC_SSP_TypeDef *SSPx, SSP_CFG_Type *SSP_ConfigStruct)
 301:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** {
 302:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	uint32_t tmp;
 303:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 304:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	CHECK_PARAM(PARAM_SSPx(SSPx));
 305:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 306:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	if(SSPx == LPC_SSP0) {
 307:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		/* Set up clock and power for SSP0 module */
 308:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCSSP0, ENABLE);
 309:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	} else if(SSPx == LPC_SSP1) {
 310:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		/* Set up clock and power for SSP1 module */
 311:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		CLKPWR_ConfigPPWR (CLKPWR_PCONP_PCSSP1, ENABLE);
 312:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	} else {
 313:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		return;
 314:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	}
 315:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 316:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	/* Configure SSP, interrupt is disable, LoopBack mode is disable,
 317:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	 * SSP is disable, Slave output is disable as default
 318:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	 */
 319:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	tmp = ((SSP_ConfigStruct->CPHA) | (SSP_ConfigStruct->CPOL) \
 320:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		| (SSP_ConfigStruct->FrameFormat) | (SSP_ConfigStruct->Databit))
 321:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		& SSP_CR0_BITMASK;
 322:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	// write back to SSP control register
 323:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	SSPx->CR0 = tmp;
 324:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	tmp = SSP_getNum(SSPx);
 325:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	if (SSP_ConfigStruct->Databit > SSP_DATABIT_8){
 326:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		sspdat[tmp].dataword = 1;
 327:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	} else {
 328:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		sspdat[tmp].dataword = 0;
 329:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	}
 330:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 331:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	tmp = SSP_ConfigStruct->Mode & SSP_CR1_BITMASK;
 332:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	// Write back to CR1
 333:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	SSPx->CR1 = tmp;
 334:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 335:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	// Set clock rate for SSP peripheral
 336:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	SSP_SetClock(SSPx, SSP_ConfigStruct->ClockRate);
 337:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** }
