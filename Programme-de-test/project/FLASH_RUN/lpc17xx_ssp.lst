 221:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @brief 		Setup clock rate for SSP device
 222:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @param[in] 	SSPx	SSP peripheral definition, should be
 223:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * 						SSP0 or SSP1.
 224:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @param[in]	target_clock : clock of SSP (Hz)
 225:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  * @return 		None
 226:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****  ***********************************************************************/
 227:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** void SSP_SetClock (LPC_SSP_TypeDef *SSPx, uint32_t target_clock)
 228:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** {
 229:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     uint32_t prescale, cr0_div, cmp_clk, ssp_clk;
 230:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 231:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     CHECK_PARAM(PARAM_SSPx(SSPx));
 232:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 233:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     /* The SSP clock is derived from the (main system oscillator / 2),
 234:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****        so compute the best divider from that clock */
 235:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     if (SSPx == LPC_SSP0){
 236:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     	ssp_clk = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_SSP0);
 237:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     } else if (SSPx == LPC_SSP1) {
 238:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     	ssp_clk = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_SSP1);
 239:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     } else {
 240:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     	return;
 241:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     }
 242:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 243:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	/* Find closest divider to get at or under the target frequency.
 244:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	   Use smallest prescale possible and rely on the divider to get
 245:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	   the closest target frequency */
 246:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	cr0_div = 0;
 247:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	cmp_clk = 0xFFFFFFFF;
 248:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	prescale = 2;
 249:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	while (cmp_clk > target_clock)
 250:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	{
 251:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		cmp_clk = ssp_clk / ((cr0_div + 1) * prescale);
 252:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		if (cmp_clk > target_clock)
 253:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		{
 254:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 			cr0_div++;
 255:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 			if (cr0_div > 0xFF)
 256:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 			{
 257:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 				cr0_div = 0;
 258:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 				prescale += 2;
 259:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 			}
 260:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 		}
 261:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 	}
 262:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** 
 263:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     /* Write computed prescaler and divider back to register */
 264:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     SSPx->CR0 &= (~SSP_CR0_SCR(0xFF)) & SSP_CR0_BITMASK;
 265:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     SSPx->CR0 |= (SSP_CR0_SCR(cr0_div)) & SSP_CR0_BITMASK;
 266:Libraries/NXP/Drivers/source/lpc17xx_ssp.c ****     SSPx->CPSR = prescale & SSP_CPSR_BITMASK;
 267:Libraries/NXP/Drivers/source/lpc17xx_ssp.c **** }
